#Challenge A Stranger Things
use_bpm 160
use_synth :bass_foundation

live_loop :beat do
  play:c2
  sleep 0.5
  play:e2
  sleep 0.5
  play:g2
  sleep 0.5
  play:b2
  sleep 0.5
  
  
  play:c3
  sleep 0.5
  play:b2
  sleep 0.5
  play:g2
  sleep 0.5
  play:e2
  sleep 0.5
end


live_loop :eerie_choir do
  1.times do
    sleep 16
    sample :elec_filt_snare, beat_stretch: 15, amp: 7
    sleep 10
    sample :glitch_bass_g, beat_stretch: 10, amp: 5
  end
end
